{
  "name": "@yourorg/audit-cli",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "audit-cli": "./dist/cli.cjs"
  },
  "files": ["dist", "README.md"],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --config ./vitest.config.ts",
    "lint": "eslint src --max-warnings 0"
  },
  "dependencies": {
    "@yourorg/audit-core": "workspace:*",
    "@yourorg/audit-export-postgres": "workspace:*",
    "pg": "^8.12.0"
  }
}
