{
  "name": "@stackio/audit-buffer-disk",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --config ./vitest.config.ts",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "exports:check": "node ../../scripts/exports-check.mjs --package @stackio/audit-buffer-disk",
    "pack:check": "node ../../scripts/pack-check.mjs --package @stackio/audit-buffer-disk"
  },
  "description": "Disk-backed buffer for audit events with replay support.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackio/audit-log.git"
  },
  "keywords": [
    "audit",
    "logging",
    "buffer",
    "disk",
    "replay"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@stackio/audit-core": "workspace:*"
  }
}
