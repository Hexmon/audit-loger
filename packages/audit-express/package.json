{
  "name": "@hexmon/audit-express",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@hexmon/audit-core": "workspace:*",
    "@hexmon/audit-node": "workspace:*"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.16",
    "supertest": "^7.2.2"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --config ./vitest.config.ts",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "exports:check": "node ../../scripts/exports-check.mjs --package @hexmon/audit-express",
    "pack:check": "node ../../scripts/pack-check.mjs --package @hexmon/audit-express"
  },
  "description": "Express middleware for audit logging with request context.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hexmon/audit-log.git"
  },
  "keywords": [
    "audit",
    "logging",
    "express",
    "middleware",
    "node"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
