{
  "name": "@yourorg/audit-express",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@yourorg/audit-core": "workspace:*",
    "@yourorg/audit-node": "workspace:*"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.16",
    "supertest": "^7.2.2"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --config ./vitest.config.ts",
    "lint": "eslint src --max-warnings 0"
  }
}
