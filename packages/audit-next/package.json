{
  "name": "@hexmon_tech/audit-next",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "@hexmon_tech/audit-core": "workspace:*"
  },
  "peerDependencies": {
    "@hexmon_tech/audit-node": "workspace:*",
    "next": "^16.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@hexmon_tech/audit-node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hexmon_tech/audit-node": "workspace:*",
    "next": "16.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest --run --config ./vitest.config.ts",
    "lint": "eslint src --max-warnings 0",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "exports:check": "node ../../scripts/exports-check.mjs --package @hexmon_tech/audit-next",
    "pack:check": "node ../../scripts/pack-check.mjs --package @hexmon_tech/audit-next"
  },
  "description": "Next.js App Router helpers for audit logging.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hexmon_tech/audit-log.git"
  },
  "keywords": [
    "audit",
    "logging",
    "nextjs",
    "app-router",
    "edge"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
